{
    "multipart": [
        {   "apply": { "model": "tubesreloaded:block/white_tube" }},
        {   "when": { "north": "true" },
            "apply": { "model": "tubesreloaded:block/white_tube_extension", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "tubesreloaded:block/white_tube_extension", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "tubesreloaded:block/white_tube_extension", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "tubesreloaded:block/white_tube_extension", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "tubesreloaded:block/white_tube_extension", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "tubesreloaded:block/white_tube_extension", "x": 90, "uvlock": true }
        }
    ]
}
